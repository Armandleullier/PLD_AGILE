      <children>
        <Button fx:id="btn_load_map" mnemonicParsing="false" onAction="#LoadMap" prefHeight="56.0" prefWidth="190.0" text="Load Map" translateX="20.0" translateY="30.0">
            <font>
                <Font size="18.0" />
            </font>
        </Button>
        <Button fx:id="btn_load_requests" disable="true" mnemonicParsing="false" onAction="#LoadRequests" prefHeight="56.0" prefWidth="290.0" text="Load Requests" translateX="800.0" translateY="30.0">
          <font>
              <Font size="18.0" />
          </font>
        </Button>
      </children>
    </HBox>
   <SplitPane dividerPositions="0.7390776699029126">
     <items>
       <AnchorPane minHeight="0.0" minWidth="0.0">
            <children>
                <Pane fx:id="overlay" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onZoom="#Zoom" prefHeight="600.0" prefWidth="1200.0">

               </Pane>
            </children>
         </AnchorPane>
       <AnchorPane fx:id="myPane" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
